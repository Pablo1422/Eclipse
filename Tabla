    private void RegistrarActionPerformed(java.awt.event.ActionEvent evt) {                                          
      Persona persona=new Persona(txtNom.getText(),txtApellid.getText(),txted.getText(),txtNum.getText());
      lista.add(persona);
      mostrar();
     txtNom.setText("");
     txtApellid.setText("");
     txted.setText("");
     txtNum.setText("");
    }                                         
    public void mostrar(){
    String matriz[][]=new String[lista.size()][4];
    for(int i=0;i<lista.size();i++){
    matriz[i][0]=lista.get(i).getNombre();
    matriz[i][1]=lista.get(i).getApellido();
    matriz[i][2]=lista.get(i).getEdad();
    matriz[i][3]=lista.get(i).getNumero();
    }
    Personal.setModel(new javax.swing.table.DefaultTableModel(
           matriz,
            new String [] {
                "Nombre", "Apellido", "Edad", "Numero"
            }
        ));
    }
   
